    </style>
    <!-- Base application theme. -->
    <style name="BtrImTheme" parent="AppTheme.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="actionBarSize">44dp</item>
        <item name="colorPrimary">#3D7EFF</item>
        <item name="colorPrimaryDark">#3D7EFF</item>
        <item name="buttonColor">#3D7EFF</item>
        <item name="colorAccent">#3D7EFF</item>
        <item name="android:textColorPrimary">@color/white</item>
        <item name="evaluateButtonTextColor">#1a3D7EFF</item>
    </style>
</resources>
